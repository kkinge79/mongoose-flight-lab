<%- include("./partials/html-head") %>

<link rel="stylesheet" href="/stylesheets/main.css" />


<%- include("./partials/nav") %>


  <h1>Flight List</h1>
  <table>
    <thead>
      <tr>
        <th>Airline:</th>
        <th>Airport:</th>
        <th>Flight No.:</th>
        <th>Departure</th>
        <th>Now Showing</th>
      </tr>
    </thead>
    <tbody>
      <% allFlights.forEach(m => { %>
        <tr>
          <td><%= m.airline %></td>
          <td><%= m.airport %></td>
          <td><%= m.mpaaRating %></td>
          <td><%= m.flightNo %></td>
          <td><%= m.departs %></td>
        </tr>
        <% }) %>
    </tbody>
  </table>

  
  <%- include("./partials/footer") %>
